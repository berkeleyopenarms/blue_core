<launch>
  <arg name="param_file" />
  <arg name="version" default="2" doc="1 or 2" />
  <arg name="on_stand" value="false" /> <!-- this value should not matter -->

  <rosparam
    file="$(arg param_file)"
    command="load" />

  <include
    file="$(find blue_bringup)/launch/include/run.launch.xml">
    <arg name="configuration" value="full" />
    <arg name="version" value="$(arg version)" />
  </include>
</launch>
